{
  "version": "1.0.0",
  "description": "Universal test framework for front-end",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=development jest --config ./scripts/jest/config.js",
    "watch": "yarn test --watch",
    "lint": "node ./scripts/eslint.js",
    "build": "node ./scripts/build.js",
    "prettier": "node ./scripts/prettier.js",
    "commit": "npx git-cz",
    "publish": "lerna publish --silent"
  },
  "private": true,
  "devEngines": {
    "node": "8.x || 9.x"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unadlib/tees.git"
  },
  "keywords": [
    "e2e",
    "test",
    "test framework"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unadlib/tees/issues"
  },
  "homepage": "https://github.com/unadlib/tees#readme",
  "devDependencies": {
    "commitizen": "^3.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^24.1.0",
    "lerna": "^2.11.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
